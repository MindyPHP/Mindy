{
    "name": "mindy/orm-slug-field",
    "homepage": "https://github.com/MindyPHP/OrmSlugField",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mindy\\Orm\\Fields\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Orm\\Fields\\Tests\\": "./Tests/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "mindy/orm": "~3.0",
        "cocur/slugify": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mindy/orm-nested-set": "~3.0"
    },
    "suggested": {
        "mindy/orm-nested-set": "Nested set for Mindy Orm"
    },
    "authors": [
        {
            "name": "Maxim Falaleev",
            "email": "max@studio107.ru",
            "homepage": "http://studio107.ru"
        }
    ],
    "support": {
        "issues": "https://github.com/MindyPHP/Mindy/issues?state=open"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.6.8-dev"
        }
    }
}
