{
    "name": "mindy/mindy",
    "type": "library",
    "description": "The Mindy PHP framework based on Symfony framework",
    "license": "MIT",
    "keywords": [
        "framework"
    ],
    "authors": [
        {
            "name": "Falaleev Maxim",
            "email": "max@mindy-cms.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mindy\\Bundle\\": "src/Mindy/Bundle/",
            "Mindy\\": "src/Mindy/",
            "Mindy\\Orm\\": [
                "src/Mindy/OrmNestedSet",
                "src/Mindy/OrmFileField"
            ],
            "Mindy\\Orm\\Fields\\": [
                "src/Mindy/OrmSlugField",
                "src/Mindy/OrmFields"
            ]
        },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "replace": {
        "mindy/mindy-bundle": "self.version",
        "mindy/table-bundle": "self.version",
        "mindy/orm-bundle": "self.version",
        "mindy/pagination-bundle": "self.version",
        "mindy/template-bundle": "self.version",
        "mindy/application": "self.version",
        "mindy/query-builder": "self.version",
        "mindy/thumb": "self.version",
        "mindy/table": "self.version",
        "mindy/finder": "self.version",
        "mindy/orm-nested-set": "self.version",
        "mindy/orm-slug-field": "self.version",
        "mindy/orm-file-field": "self.version",
        "mindy/orm-fields": "self.version",
        "mindy/orm": "self.version",
        "mindy/pagination": "self.version",
        "mindy/template": "self.version"
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/config": "~3.0",
        "symfony/dependency-injection": "~3.0",
        "symfony/framework-bundle": "~3.0",
        "symfony/routing": "~3.0",
        "symfony/http-kernel": "~3.0",
        "symfony/expression-language": "~3.0",
        "doctrine/dbal": "~2.0",
        "symfony/validator": "~3.0",
        "cocur/slugify": "~2.0",
        "league/flysystem": "~1.0",
        "imagine/imagine": "~0.6"
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "3.6.8-dev"
        }
    }
}
